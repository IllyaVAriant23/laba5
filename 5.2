#include <stdio.h>

int main() {
    int n, x;
    printf("Введіть розмір масиву: ");
    scanf("%d", &n);
    
    int a[n];
    
    printf("Введіть ціле число x: ");
    scanf("%d", &x);
    
    printf("Введіть елементи масиву:\n");
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
    
    int sum = 0;
    for (int i = 0; i < x; i++) {
        sum += a[i] * a[i];
    }
    
    printf("Сума квадратів перших %d елементів масиву: %d\n", x, sum);
    
    return 0;
}
